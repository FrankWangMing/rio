{
  "name": "@rio/layers",
  "description": "A Photoshop-like layers panel for your page editor",
  "keywords": [
    "wysiwyg",
    "html",
    "layers",
    "photoshop",
    "craftjs",
    "page-editor",
    "web-builder"
  ],
  "version": "0.2.6",
  "author": "Prev Wong <prevwong@gmail.com>",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/types/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "pnpm run start -w",
    "start": "cross-env NODE_ENV=development rollup -c",
    "build": "cross-env NODE_ENV=production rollup -c",
    "clean": "rimraf dist"
  },
  "dependencies": {
    "@rio/core": "workspace:*",
    "@rio/utils": "workspace:*",
    "react-contenteditable": "^3.3.7"
  },
  "devDependencies": {
    "@rio/common": "workspace:*",
    "@rio/eslint-config": "workspace:*",
    "@svgr/rollup": "8.1.0",
    "styled-components": "6.1.13",
    "react-dom": "^18.0.0",
    "react": "^18.0.0"
  },
  "peerDependencies": {
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "react": "^16.8.0 || ^17 || ^18",
    "react-dom": "^16.8.0 || ^17 || ^18",
    "styled-components": ">= 6.1"
  }
}
